{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python"
        },
        {
            "src": "api/*.py",
            "use": "@vercel/python"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/generate",
            "dest": "/index.py"
        },
        {
            "src": "/api/generate_kml",
            "dest": "/api/generate_kml.py"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}